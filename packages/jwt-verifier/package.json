  "engines": {
    "node": ">=6"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "jsdom"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@okta/configuration-validation": "^1.0.0",
    "jwks-rsa": "1.12.1",
    "njwt": "^1.0.0"
  },
  "devDependencies": {
    "cors": "^2.8.4",
    "cross-env": "^5.1.1",
    "express": "^4.15.4",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "nock": "^9.0.14",
    "node-fetch": "^2.6.0",
    "timekeeper": "^1.0.0",
    "typescript": "^4.1.5"
  }
}
