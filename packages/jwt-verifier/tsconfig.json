{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "noEmit": false,
    "strict": false,
    "alwaysStrict": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": false,
    "moduleResolution": "node",
    "target": "es2017",
    "baseUrl": ".",
    "paths": {
      "@okta/jwt-verifier": [
        "./lib"
      ]
    }
  },
  "include": [
    "lib.js",
    "lib.d.ts",
  ]
}