{
  "name": "@okta/oidc-middleware",
  "version": "0.0.13",
  "description": "OpenId Connect middleware for authorization code flows",
  "repository": "https://github.com/okta/okta-oidc-js",
  "homepage": "https://github.com/okta/okta-oidc-js/tree/master/packages/oidc-middleware",
  "keywords": [
    "okta",
    "oidc",
    "OpenId Connect",
    "authentication",
    "auth"
  ],
  "main": "index.js",
  "config" : { 
    "tck": {
      "jar": "https://oss.sonatype.org/service/local/artifact/maven/redirect?r=snapshots&g=com.okta.tests&a=okta-oidc-tck&v=0.2.0-SNAPSHOT&e=jar&c=shaded",   
      "setup_env": "export ISSUER=http://localhost:9090/oauth2/default export CLIENT_ID=OOICU812 export CLIENT_SECRET=VERY_SECRET",
      "command": "java -cp ./test/integration-test/resources/:./okta-oidc-tck-0.2.0-SNAPSHOT-shaded.jar org.testng.TestNG -d target/cli-test-output ./test/integration-test/resources/testng.xml"
    }
  },
  "scripts": {
    "test": "webdriver-manager update && protractor test/e2e/conf.js",
    "tck-test": "(ls ./okta-oidc-tck-0.2.0-SNAPSHOT-shaded.jar || curl $npm_package_config_tck_jar -L -o okta-oidc-tck-0.2.0-SNAPSHOT-shaded.jar) && $npm_package_config_tck_setup_env && mkdir -p target && $npm_package_config_tck_command || echo done"
  },
  "engines": {
    "node": ">=6"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-ensure-login": "^0.1.1",
    "csurf": "^1.9.0",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "negotiator": "^0.6.1",
    "openid-client": "^1.12.0",
    "passport": "^0.3.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ejs": "^2.5.7",
    "express-session": "^1.15.5",
    "protractor": "^5.1.2",
    "server-destroy": "^1.0.1",
    "webdriver-manager": "^12.0.6"
  }
}
