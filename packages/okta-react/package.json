{
  "name": "@okta/okta-react",
  "version": "1.3.0",
  "description": "React support for Okta",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist/ && yarn build:package-info && babel src -d dist",
    "build:package-info": "node ../../util/write-package-info.js . src/packageInfo.js",
    "lint": "eslint src",
    "lint:watch": "esw -w lib/**",
    "prepare": "yarn build",
    "start": "yarn --cwd test/e2e/harness start",
    "test": "yarn lint && yarn test:unit && yarn test:e2e",
    "test:e2e": "yarn --cwd test/e2e/harness test",
    "test:unit": "jest test/jest/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okta/okta-oidc-js.git"
  },
  "keywords": [
    "oauth2"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/okta/okta-oidc-js/issues"
  },
  "homepage": "https://github.com/okta/okta-oidc-js#readme",
  "dependencies": {
    "@okta/configuration-validation": "^0.4.1",
    "@okta/okta-auth-js": "^2.11.0",
    "babel-runtime": "^6.26.0",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-router": ">=4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.0.2",
    "enzyme": "^3.5.1",
    "enzyme-adapter-react-16": "^1.4.0",
    "eslint": "^4.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^23.6.0",
    "polished": "^1.7.0",
    "protractor": "^5.4.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "styled-components": "^2.1.2",
    "webdriver-manager": "^12.1.4"
  },
  "resolutions": {
    "webdriver-manager": "^12.1.4"
  },
  "jest": {
    "setupFiles": [
      "./test/jest/setup.js"
    ],
    "testEnvironment": "jsdom"
  }
}
