{
  "name": "@okta/okta-react",
  "version": "0.0.2",
  "description": "React support for Okta",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist/ && NODE_ENV=development babel src -d dist",
    "lint": "eslint src/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "prestart": "npm run build",
    "start": "npm run --prefix test/e2e/harness start",
    "pretest": "npm run build",
    "test": "npm run --prefix test/e2e/harness test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okta/okta-oidc-js.git"
  },
  "keywords": [
    "oauth2"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/okta/okta-oidc-js/issues"
  },
  "homepage": "https://github.com/okta/okta-oidc-js#readme",
  "dependencies": {
    "@okta/okta-auth-js": "^1.8.0",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": ">=15",
    "react-router": ">=4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "eslint": "^4.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "polished": "^1.7.0",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "styled-components": "^2.1.2"
  }
}
